{
  "name": "zeroseven/font-loader",
  "license": "GPL-2.0-only",
  "type": "typo3-cms-extension",
  "description": "Load Google Fonts and Fontawesome to share from your TYPO3",
  "authors": [
    {
      "name": "zeroseven design studios GmbH",
      "email": "typo3@zeroseven.de",
      "homepage": "https://www.zeroseven.de/",
      "role": "Company"
    },
    {
      "name": "Raphael Thanner",
      "role": "Developer"
    }
  ],
  "require": {
    "typo3/cms-core": "^10.4 || ^11.5"
  },
  "autoload": {
    "psr-4": {
      "Zeroseven\\FontLoader\\": "Classes"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "font_loader"
    }
  },
  "scripts": {
    "prepare-release": [
      "rm .editorconfig",
      "rm .gitattributes",
      "rm .gitignore",
      "rm .gitlab-ci.yml",
      "rm -rf .github"
    ]
  }
}
